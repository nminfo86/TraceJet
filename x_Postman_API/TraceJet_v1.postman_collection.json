	"item": [
		{
			"name": "Authentication",
			"item": [
				{
					"name": "Create access token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var current_app_api_key=\"B@lmsieHd\";\r",
									"\r",
									"pm.test('api-key should exist and equal current app key', function () {\r",
									"    if (pm.environment.has('api-key') ) {\r",
									"        if(pm.expect(pm.environment.get('api-key')).to.equal(current_app_api_key)){\r",
									"           console.log(current_app_api_key)\r",
									"        }\r",
									"    };\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Token is created\", function () {\r",
									"    pm.response.to.have.status(201);\r",
									"    pm.environment.set(\"token\", pm.response.json().token)\r",
									"});\r",
									"   \r",
									"\r",
									"\r",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"protocolProfileBehavior": {
						"disabledSystemHeaders": {}
					},
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [
							{
								"key": "Accept",
								"value": "application/json",
								"type": "default",
								"disabled": true
							}
						],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "username",
									"value": "admin",
									"description": "Required",
									"type": "default"
								},
								{
									"key": "password",
									"value": "123123",
									"description": "Required| min 6",
									"type": "default"
								}
							]
						},
						"url": {
							"raw": "{{host}}/auth/access_token",
							"host": [
								"{{host}}"
							],
							"path": [
								"auth",
								"access_token"
							]
						}
					},
					"response": []
				},
